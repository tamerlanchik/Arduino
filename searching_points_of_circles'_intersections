def f(x1, y1, R1, R2):
    e=(R2**2-R1**2-x1**2-y1**2)/(-2)

    a=y1**2+x1**2
    b=(-2)*e*y1
    c=e**2-(R1**2)*(x1**2)

    D=b**2-4*a*c

    if D<0: print('Не пересекаются')
    elif D==0:
        y=(-b)/(2*a)
        x=(e-y*y1)/x1
        print((x, y))
    elif D>0:
        Y1=((-b)+D**0.5)/(2*a)
        Y2=((-b)-D**0.5)/(2*a)
        
        X1=(e-Y1*y1)/x1
        X2=(e-Y2*y1)/x1
        print((X1, Y1), (X2, Y2))
    else:
        print('Error')

f(8, 6, 10, 5)
